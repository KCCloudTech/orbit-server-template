{
  "name": "orbit-server-template",
  "version": "0.5.0",
  "author": "Paul Chavard",
  "license": "MIT",
  "dependencies": {
    "@orbit-server/fastify": "^0.7.3",
    "fastify": "^2.7.1",
    "fastify-cli": "^1.2.0",
    "fastify-plugin": "^1.6.0",
    "inflected": "^2.0.4",
    "orbit-sql": "^0.1.1",
    "sqlite3": "^4.1.0"
  },
  "scripts": {
    "routes": "yarn fastify print-routes ./dist/app.js",
    "postinstall": "tsc",
    "start": "tsc-watch --onSuccess \"yarn start:dev\"",
    "start:dev": "fastify start -l info -P dist/app.js",
    "start:heroku": "fastify start -a '0.0.0.0' -l info dist/app.js"
  },
  "engines": {
    "node": "12.x"
  },
  "devDependencies": {
    "@types/inflected": "^1.1.29",
    "@types/node": "^12.7.2",
    "tsc-watch": "^2.4.0",
    "typescript": "^3.5.3"
  }
}
