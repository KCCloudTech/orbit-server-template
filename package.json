{
  "name": "orbit-server-template",
  "version": "0.5.0",
  "author": "Paul Chavard",
  "license": "MIT",
  "dependencies": {
    "@orbit-server/fastify": "^0.6.0",
    "fastify": "^2.6.0",
    "fastify-cli": "^1.1.0",
    "fastify-plugin": "^1.6.0",
    "inflected": "^2.0.4",
    "sqlite3": "^4.0.9"
  },
  "scripts": {
    "postinstall": "tsc",
    "start": "tsc-watch --onSuccess \"yarn start:dev\"",
